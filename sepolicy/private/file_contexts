# Data files
/data/display(/.*)?      u:object_r:display_data_file:s0

# Files in rootfs
/bt_firmware(/.*)?       u:object_r:bt_firmware_file:s0
/dsp(/.*)?               u:object_r:adsprpcd_file:s0
/firmware(/.*)?          u:object_r:firmware_file:s0
/persist(/.*)?           u:object_r:persist_file:s0

# Sysbta
/system/bin/hw/android\.hardware\.bluetooth\.audio-service-system    u:object_r:hal_audio_sysbta_exec:s0

# Xiaomi touch
/system/bin/xiaomi-touch     u:object_r:xiaomi_touch_exec:s0
/dev/xiaomi-touch            u:object_r:touchfeature_device:s0

# disp
/dev/mi_display/disp_feature u:object_r:touchfeature_device:s0
/sys/devices/virtual/mi_display/disp_feature/disp-DSI-+[0-1](/.*)? u:object_r:sysfs_displayfeature:s0
